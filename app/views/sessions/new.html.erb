<h1>Log in</h1>
<%= form_tag sessions_path do %>

  <div class="field">
    <%= label_tag :email %>
    <%= email_field_tag :email %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="submit">
    <%= button_tag "Log in" %>
  </div>

<% end %>
